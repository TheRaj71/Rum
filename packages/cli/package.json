{
  "name": "rumm",
  "version": "1.0.2",
  "description": "rumm - A delightful CLI for Svelte 5 component registry. Ship beautiful components with ease.",
  "type": "module",
  "bin": {
    "rumm": "./dist/index.js"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./schema": {
      "types": "./dist/schema/index.d.ts",
      "import": "./dist/schema/index.js"
    },
    "./utils": {
      "types": "./dist/utils/index.d.ts",
      "import": "./dist/utils/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "keywords": [
    "svelte",
    "svelte5",
    "components",
    "registry",
    "cli",
    "shadcn",
    "ui"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/TheRaj71/Rum"
  },
  "homepage": "https://github.com/TheRaj71/Rum#readme",
  "bugs": {
    "url": "https://github.com/TheRaj71/Rum/issues"
  },
  "license": "MIT",
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "lint": "eslint src/",
    "format": "prettier --write src/",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "commander": "^13.1.0",
    "magic-string": "^0.30.17",
    "ora": "^9.0.0",
    "picocolors": "^1.1.1",
    "svelte": "^5.0.0",
    "zod": "^3.25.67"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/estree": "^1.0.8",
    "@types/node": "^22.15.29",
    "eslint": "^9.39.1",
    "fast-check": "^4.1.1",
    "globals": "^16.5.0",
    "prettier": "^3.7.4",
    "tsup": "^8.5.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.48.1",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
